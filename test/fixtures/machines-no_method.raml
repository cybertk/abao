#%RAML 0.8

title: Machines API
baseUri: http://example.api.com/{version}
version: v1

/root:
  /machines:
    get:
      description: Gets a list of existing machines
      responses:
        200:
          body:
            application/json:
              schema: |
                [
                  type: 'string'
                  name: 'string'
                ]
              example: |
                { "type": "Kulu", "name": "Mike" }

