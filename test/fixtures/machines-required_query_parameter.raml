#%RAML 0.8

title: Machines API
baseUri: http://example.api.com/{version}
version: v1

/machines:
  get:
    description: Gets a list of existing machines
    queryParameters:
      quux:
        type: string
        required: true
        example: foo
    responses:
      200:
        body:
          application/json:
            schema: |
              [
                type: 'string'
                name: 'string'
              ]
            example: |
              { "type": "Kulu", "name": "Mike" }

