#%RAML 0.8

title: Machines API
baseUri: http://example.api.com/{version}
version: v1

/machines:
  get:
    responses:
      200:
        body:
          application/json:
            schema: |
              [
                type: 'string'
                name: 'string'
              ]
  /{machine_id}:
    uriParameters:
      machine_id:
        description: |
          The ID of the Machine
        type: string
        example: '1'
    delete:
      responses:
        204:
    /parts:
      get:
        responses:
          200:
            body:
              application/json:
                schema: |
                  type: 'string'
                  name: 'string'

