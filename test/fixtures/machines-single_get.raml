#%RAML 0.8

title: Machines API
baseUri: http://localhost:3333

/machines:
  get:
    description: Gets a list of existing machines
    headers:
      Abao-API-Key:
        type: string
        example: abcdef
    responses:
      200:
        body:
          application/json:
            schema: |
              [
                type: 'string'
                name: 'string'
              ]
            example: |
              { "type": "Kulu", "name": "Mike" }

